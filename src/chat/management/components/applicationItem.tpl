<div w-class="applicationItem">
    <img src="{{it.icon}}" alt="" w-class="appIcon"/>
    <div w-class="appInfo">
        <div w-class="appName">{{it.name}}</div>
        <div w-class="appDesc">{{it.desc}}</div>
        <div w-class="appId">
            <div w-class="idItem">客服ID：{{it.customerId}}</div>
            <div w-class="idItem">应用ID：{{it.appId}}</div>
            <div>添加时间：{{it.time}}</div>
        </div>
    </div>
</div>