.header{
    border-top: 1px solid #ccc;
    border-left: 1px solid #ccc;
}
.body{
    max-height: 400px;
    overflow-x: hidden;
    overflow-y: auto;
    border-left: 1px solid #ccc;
}
.table{
    table-layout: fixed;
    border-collapse: collapse;
    min-width: 100%;
}
.has-gutter{
    color: #909399;
    font-weight: 500;
}
.th{
    padding: 10px;
    line-height: 25px;
    text-align: center;
    border-bottom: 1px solid #ccc;
    border-right: 1px solid #ccc;
}
.td{
    transition: background-color .25s ease;
    padding: 10px;
    line-height: 25px;
    text-align: center;
    border-bottom: 1px solid #ccc;
    border-right: 1px solid #ccc;
    WORD-BREAK: break-all;
}
.th1{
    padding: 0;
}
.td1{
   padding: 0;
   display: flex;
   justify-content: space-around;
}
.bottom{
    height:48px;
    background:rgba(240,240,240,1);
    line-height: 48px;
    display: flex;
    justify-content: space-between;
    border: 1px solid #CCC;
    border-top: none;
}
.allCheck{
    display: flex;
    align-items: center;
    width: 100px;
    padding-left: 30px;
    box-sizing: border-box;
    text-align: right;
}
.btn{
    height:36px;
    color: #21811C;
    text-align: center;
    line-height: 36px;
    font-size: 14px;
    display: inline-block;
    cursor: pointer;
}
.btnColor{
    color: red;
    padding: 0 10px;
}
.input{
    appearance:none;
    -moz-appearance:none; /* Firefox */
    -webkit-appearance:none; /* Safari å’Œ Chrome */
}
.dealBtn{
    width:100px;
    font-size:14px;
    color:rgba(33,129,28,1);
    text-decoration: underline;
    text-align: center;
}
.btns{
    margin-left: 20px;
}
.bottomBtn{
    width:100px;
    background:rgba(25,145,235,1);
    height:36px;
    color: #fff;
    text-align: center;
    line-height: 36px;
    font-size: 14px;
    display: inline-block;
}
.box{
    overflow: auto;
}
.autoTd{
    white-space: nowrap;
    transition: background-color .25s ease;
    padding: 10px;
    line-height: 25px;
    text-align: center;
    border-bottom: 1px solid #ccc;
    border-right: 1px solid #ccc;
}
.autoTh{
    white-space: nowrap;
    padding: 10px;
    line-height: 25px;
    text-align: center;
    border-bottom: 1px solid #ccc;
    border-right: 1px solid #ccc;
}
.autoBody{
    max-height: 625px;
    border-left: 1px solid #ccc;
}
.exportFile{
    position: relative;
}
.btn1{
    position: absolute;
    top:-11px;
    left: 50%;
    margin-left: -75px;
    width:150px;
    opacity: 0;
}
.exportFileBtn{
    position: absolute;
    top:-11px;
    left: 50%;
    margin-left: -75px;
    width:150px;
    color:white;
    background: rgb(25, 145, 235);
}