.reportBox{
    position: absolute;
    top: 0;
    bottom: 0;
    right: 0;
    left: 0;
    padding: 0 75px;
    display: flex;
    justify-content: center;
    align-items: center;
}
.body{
    width: 700px;
    height: 500px;
    background:rgba(255,255,255,1);
    box-shadow:0px 0px 10px 0px rgba(0,0,0,0.2);
    border-radius:4px;
    display: flex;
    flex-direction: column;
}
.reportTitle{
    height: 82px;
    margin: 30px;
    font-size: 16px;
    font-family: SourceHanSansCN;
    font-weight: 700;
    color: rgb(53, 64, 82);
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}
.userInfo{
    display: flex;
    align-items: center;
}
.avatar{
    width: 24px;
    height: 24px;
}
.userName{
    font-size:14px;
    font-family:SourceHanSansCN;
    font-weight:400;
    color:rgba(34,34,34,1);
    margin-left: 7px;
}
.checkType{
    height: 55px;
    margin: 0 30px;
    background:rgba(242,242,242,1);
    border:1px solid rgba(204,204,204,1);
    display: flex;
    font-size:14px;
    font-family:SourceHanSansCN;
    font-weight:400;
    color:rgba(53,64,82,1);
}
.typeList{
    height: 93px;
    font-size:14px;
    font-family:SourceHanSansCN;
    font-weight:400;
    color:rgba(53,64,82,1);
    margin: 0 30px;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    justify-content: space-evenly;
}
.row1{
    width: 103px;
    display: flex;
    margin-left: 20px;
    align-items: center;
    cursor: pointer;
}
.rowItem{
    margin-left: 10px;
}
.rowImg{
    width: 16px;
    height: 16px;
}
.checkBox{
    display: flex;
}
.sendNotice{
    font-size:14px;
    font-family:SourceHanSansCN;
    font-weight:400;
    color:#D39853;
    margin: 30px;
}
.btnGroup{
    display: flex;
    margin: 20px 0;
    justify-content: space-around;
}
.btn{
    width:86px;
    height:36px;
    background: #1991EB;
    font-size:14px;
    border-radius:4px;
    font-family:SourceHanSansCN;
    font-weight:400;
    color:white;
    text-align: center;
    line-height: 36px;
    cursor: pointer;
}
.haihaiName{
    width:40px;
    height:16px;
    border-radius:8px;
    border:1px solid rgba(49,91,200,1);
    font-size:12px;
    font-family:STHeitiSC;
    font-weight:300;
    color:rgba(49,91,200,1);
    text-align: center;
    line-height: 16px;
    margin-left: 10px;
    padding: 2px;
}