.message-set-wrap{
    margin: 0;
    width:100%;
    height:60px;
    display:flex;
    align-items:center;
    padding:10px 20px;
    background:rgba(0,0,0,.6);
}
.li-item{
    list-style:none;
    width: 130px;
    height:30px;
    line-height:30px;
    padding:5px 10px;
    outline:solid gray 1px;
    text-align:center;
    font-size:28px;
    color:white;
}