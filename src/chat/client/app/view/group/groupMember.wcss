.search-input{
    position:relative;
    margin-left: 20px;
    margin-top: 30px;
    width: 710px;
    height: 110px;
    border-radius: 12px;
    background-color: rgba(255, 255, 255, 1);
    box-shadow: 0px 0px 2px 0px rgba(0, 0, 0, 0.2);
}
.searchIcon{
    position:absolute;
    left:30px;
    top:40px;
    width:32px;
    height:32px;
}
.member-wrap{
    margin: 0 20px 30px;
    border-radius: 12px;
    background-color: rgba(255, 255, 255, 1);
    box-shadow: 0px 0px 2px 0px rgba(0, 0, 0, 0.2);
    display:flex;
    flex-wrap: wrap;
    justify-content: space-between;
}
.content{
    flex: 1 0 0;
    overflow-x: hidden;
    overflow-y: auto;
    margin-top: 30px;
}