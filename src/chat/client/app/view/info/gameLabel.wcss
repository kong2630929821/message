.listLabel{
    display: flex;
    flex-wrap: wrap;
    margin: 10px 0 0 10px;
}
.labelItem{
    height: 60px;
    padding: 10px;
    background:rgba(242,242,242,1);
    border-radius:6px;
    font-size:28px;
    font-family:SourceHanSansCN-Normal,SourceHanSansCN;
    font-weight:400;
    color:rgba(34,34,34,1);
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 20px 0 0 20px;
}
.checkLabelIcon{
    width: 40px;
    height: 40px;
    margin-right: 5px;
}