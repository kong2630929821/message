.page{
    background: #F9F9F9;
}
.item{
    height:100px;
    background:rgba(255,255,255,1);
    display: flex;
    align-items: center;
    padding: 0 40px;
    background: #fff;
}
.itemName{
    font-size:32px;
    font-weight:500;
    color:rgba(34,34,34,1);
    flex: 1 0 0;
}
.itemDesc{
    font-size:28px;
    font-weight:300;
    color:rgba(136,136,136,0.87);
}
.divide{
    height: 20px;
}